	journal = {Proceedings of the National Academy of Sciences},
	author = {Chiorazzi, Michael and Rui, Lixin and Yang, Yandan and Ceribelli, Michele and Tishbi, Nima and Maurer, Carine W. and Ranuncolo, Stella M. and Zhao, Hong and Xu, Weihong and Chan, Wing-Chung C. and Jaffe, Elaine S. and Gascoyne, Randy D. and Campo, Elias and Rosenwald, Andreas and Ott, German and Delabie, Jan and Rimsza, Lisa M. and Shaham, Shai and Staudt, Louis M.},
	month = mar,
