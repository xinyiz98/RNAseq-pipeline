	pages = {268--281},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\xinyi\\Zotero\\storage\\8GRBWFZK\\Jiang et al. - 2014 - A Microtubule-Associated Zinc Finger Protein, BuGZ.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\xinyi\\Zotero\\storage\\IT9TB7FE\\S1534580713007600.html:text/html}
