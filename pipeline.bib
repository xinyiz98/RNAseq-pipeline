@article{corazzari_endoplasmic_2017,
	title = {Endoplasmic {Reticulum} {Stress}, {Unfolded} {Protein} {Response}, and {Cancer} {Cell} {Fate}},
	volume = {7},
	issn = {2234-943X},
	url = {https://www.frontiersin.org/articles/10.3389/fonc.2017.00078/full},
	doi = {10.3389/fonc.2017.00078},
	abstract = {Perturbation of endoplasmic reticulum (ER) homeostasis results in a stress condition termed ‘ER stress’ determining the activation of a finely regulated program defined as Unfolded Protein Response (UPR) and whose primary aim is to restore this organelle’s physiological activity. Several physiological and pathological stimuli deregulate normal ER activity causing UPR activation, such as hypoxia, glucose shortage, genome instability and cytotoxic compounds administration. Some of these stimuli are frequently observed during uncontrolled proliferation of transformed cells, resulting in tumour core formation and stage progression. Therefore, it is not surprising that ER stress is usually induced during solid tumour development and stage progression, becoming an hallmark of such malignancies. Several UPR components are in fact deregulated in different tumour types and accumulating data indicate their active involvement in tumour development/progression. However, although the UPR program is primarily a pro-survival process, sustained and/or prolonged stress may result in cell death induction. Therefore, understanding the mechanism(s) regulating the cell survival/death decision under ER stress condition may be crucial in order to specifically target tumour cells and possibly circumvent or overcome tumour resistance to therapies. In this review, we discuss the role played by the UPR program in tumor initiation, progression and resistance to therapy, highlighting the recent advances that have improved our understanding of the molecular mechanisms that regulate the survival/death switch.},
	language = {English},
	urldate = {2018-03-01},
	journal = {Frontiers in Oncology},
	author = {Corazzari, Marco and Gagliardi, Mara and Fimia, Gian Maria and Piacentini, Mauro},
	year = {2017},
	keywords = {er stress, upr, E2F, Autophagy, BRAF},
	file = {Full Text PDF:C\:\\Users\\xinyi\\Zotero\\storage\\533AE69F\\Corazzari et al. - 2017 - Endoplasmic Reticulum Stress, Unfolded Protein Res.pdf:application/pdf}
}

