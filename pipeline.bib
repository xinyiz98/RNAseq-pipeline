	keywords = {Gene expression, Animals, Corpus Striatum, Gene Expression Profiling, High-Throughput Nucleotide Sequencing, Mice, Mice, Inbred C57BL, Mice, Inbred DBA, Oligonucleotide Array Sequence Analysis, Polymorphism, Single Nucleotide, Sequence Analysis, RNA},
