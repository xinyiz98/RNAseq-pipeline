	file = {Full Text PDF:C\:\\Users\\xinyi\\Zotero\\storage\\2FA9T7WI\\Julien et al. - 2014 - Unraveling the mechanism of cell death induced by .pdf:application/pdf;Snapshot:C\:\\Users\\xinyi\\Zotero\\storage\\8HKIFT9F\\nchembio.html:text/html}
}

