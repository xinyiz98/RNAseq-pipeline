@article{converse_integrating_2016,
	title = {Integrating self-control with physical attractiveness and cognitive ability to examine pathways to career success},
	volume = {89},
	issn = {2044-8325},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/joop.12107/abstract},
	doi = {10.1111/joop.12107},
	abstract = {Physical attractiveness, cognitive ability, and self-control predict many important outcomes but are rarely examined in combination, particularly in the context of career success. Drawing from Judge et al.'s (2009, J. Appl. Psychol., 94, 742) model of income antecedents and Caspi et al.'s (1989, J. Pers., 57, 375) concepts of cumulative and interactional continuity, this study investigated the pathways leading from these characteristics to career outcomes. Specifically, we developed a model with links involving (1) attractiveness, intelligence, and self-control; (2) self-evaluations, delinquent behaviours, educational attainment, and job complexity; and (3) income and job satisfaction. The model was examined with data from three waves of the National Longitudinal Study of Adolescent Health. Results supported a slightly modified model, with major findings indicating attractiveness, intelligence, and self-control measured during adolescence predicted later self-evaluations or delinquent behaviour; these variables predicted later education level; education level predicted job complexity; and job complexity predicted income and job satisfaction. Implications related to this integrated model are discussed.


Practitioner points





* Interventions might focus on enhancing self-control-related skills in adolescents in an effort to improve later work-related outcomes.


* Core self-evaluations and delinquent tendencies could also be targeted during adolescence to influence life pathways relevant to career outcomes.},
	language = {en},
	number = {1},
	urldate = {2016-11-27},
	journal = {Journal of Occupational and Organizational Psychology},
	author = {Converse, Patrick D. and Thackray, Michelle and Piccone, Katrina and Sudduth, Mary Margaret and Tocci, Michael C. and Miloslavic, Stephanie A.},
	month = mar,
	year = {2016},
	keywords = {career success, physical attractiveness, cognitive ability, self-control},
	pages = {73--91},
	file = {Full Text PDF:C\:\\Users\\xinyi\\Zotero\\storage\\ZWQ5WF2E\\Converse et al. - 2016 - Integrating self-control with physical attractiven.pdf:application/pdf;Snapshot:C\:\\Users\\xinyi\\Zotero\\storage\\KE3I57H2\\abstract.html:text/html}
}

